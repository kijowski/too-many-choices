{
  "compilerOptions": {
    "target": "es5",
    "lib": ["DOM", "ESNext"],
    "module": "ESNext",
    "jsx": "react",
    "jsxFactory": "h",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true /* Disallow inconsistently-cased references to the same file. */
  },
  "include": ["src"]
}
