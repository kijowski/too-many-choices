{
  "compilerOptions": {
    "target": "es5",
    "lib": ["DOM", "ESNext"],
    "module": "ESNext",
    "jsx": "react",
    "jsxFactory": "h",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["typings", "node_modules/@types"]
  },
  "include": ["src", "api", "typings"]
}
